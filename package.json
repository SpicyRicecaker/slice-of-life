{
  "private": "true",
  "workspaces": [
    "pages"
  ],
  "name": "slice-of-life",
  "productName": "slice-of-life",
  "version": "0.4.0",
  "description": "Visualizer for time spent in a day using chart.js",
  "main": "dist/index.js",
  "scripts": {
    "dev":"concurrently \"yarn:start\" \"lerna run start\"",
    "start": "tsc && electron-forge start",
    "package": "lerna run build && tsc && electron-forge package",
    "make": "lerna run build && tsc && electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts ."
  },
  "keywords": [],
  "author": {
    "name": "SpicyRicecaker",
    "email": "awesomenessisgolden@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "slice_of_life"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "electron": "11.0.3",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "^2.20.0",
    "lerna": "^3.22.1",
    "typescript": "^4.0.2",
    "concurrently": "^5.3.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  }
}
